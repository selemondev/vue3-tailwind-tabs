{
  "name": "vue3-tailwind-tabs",
  "private": false,
  "version": "0.1.7",
  "description": "A collection of Vue 3 components for creating tabs with Tailwind CSS.",
  "author": "Selemon Brahanu",
  "keywords": [
    "vue",
    "tailwindcss",
    "typescript",
    "tabs",
    "components"
  ],
  "files": [
    "dist"
  ],
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/selemondev/vue3-tailwind-tabs"
  },
  "bugs": {
    "url": "https://github.com/selemondev/vue3-tailwind-tabs"
  },
  "main": "./dist/vue3-tailwind-tabs.umd.js",
  "module": "./dist/vue3-tailwind-tabs.es.js",
  "exports": {
    ".": {
      "types": "./dist/components/build.d.ts",
      "import": "./dist/vue3-tailwind-tabs.es.js",
      "require": "./dist/vue3-tailwind-tabs.umd.js"
    },
    "./dist/build.css": {
      "import": "./dist/build.css",
      "require": "./dist/build.css"
    }
  },
  "types": "./dist/components/build.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build && vue-tsc --emitDeclarationOnly && mv dist/src dist/types",
    "preserve": "vite build",
    "serve": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "exit 0"
  },
  "peerDependencies": {
    "vue": "^3.2.47"
  },
  "dependencies": {
    "classnames": "^2.3.2",
    "tailwind-scrollbar-hide": "^1.1.7"
  },
  "devDependencies": {
    "@iconify/vue": "^4.1.1",
    "@types/node": "^20.4.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.25",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6",
    "vite": "^4.4.2",
    "vite-plugin-dts": "^3.1.1",
    "vue-tsc": "^1.8.4"
  }
}